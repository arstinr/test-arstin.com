{
    "version": 2,
    "outputDirectory": ".",
    "builds": [
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/videos/(.*)",
        "headers": { "cache-control": "public, max-age=31536000, immutable" },
        "dest": "/public/videos/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
}